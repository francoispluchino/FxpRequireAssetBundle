<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="twig.extension.fxp_require_asset.class">Fxp\Component\RequireAsset\Twig\Extension\AssetExtension</parameter>
    </parameters>

    <services>
        <service id="twig.extension.fxp_require_asset" class="%twig.extension.fxp_require_asset.class%" public="false">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
