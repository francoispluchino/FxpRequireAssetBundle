{
    "name": "fxp/require-asset-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle helper of required assets for Twig with Webpack",
    "keywords": ["asset", "require", "webpack", "twig", "javascript", "stylesheet", "user interface", "ui", "symfony", "bundle"],
    "homepage": "https://github.com/fxpio/fxp-require-asset-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^3.4.0|^4.0.0",
        "symfony/asset": "^3.4.0|^4.0.0",
        "symfony/expression-language": "^3.4.0|^4.0.0",
        "fxp/require-asset": "^1.0.0"
    },
    "require-dev": {
        "symfony/twig-bundle": "^3.4.0|^4.0.0"
    },
    "suggest": {
        "foxy/foxy": "To inject the NPM dependencies of PHP libraries in project NPM dependencies",
        "fxp/composer-asset-plugin": "To manage NPM/Bower dependencies with Composer"
    },
    "autoload": {
        "psr-4": { "Fxp\\Bundle\\RequireAssetBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
