{
    "name": "fxp/require-asset-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for required assets helper for assetic manager and twig",
    "keywords": ["asset", "require", "twig", "assetic", "javascript", "stylesheet", "user interface", "ui", "symfony", "bundle"],
    "homepage": "https://github.com/fxpio/fxp-require-asset-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/assetic-bundle": "~2.8",
        "symfony/twig-bundle": "~3.0",
        "symfony/expression-language": "~3.0",
        "fxp/require-asset": "1.0.*@dev"
    },
    "suggest": {
        "fxp/composer-asset-plugin": "For manage NPM/Bower dependencies with Composer"
    },
    "autoload": {
        "psr-4": { "Fxp\\Bundle\\RequireAssetBundle\\": "" },
		"exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
